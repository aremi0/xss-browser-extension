{
  "manifest_version": 2,
  "name": "XSS SearchBar",
  "version": "1.0",
  "description": "Shitty searchbar -with body color-changer- that injest harmless JS code",
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_title": "XSS SearchBar",
    "default_icon": "img/idea.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/idea.png",
    "32": "img/idea.png",
    "48": "img/idea.png",
    "128": "img/idea.png"
  }
}